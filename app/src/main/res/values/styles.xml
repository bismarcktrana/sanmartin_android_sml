<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextLabel555" parent="TextAppearance.AppCompat">
        <item name="android:textColorHint">@color/colorAccent</item>
        <item name="android:textSize">@dimen/cell_height</item>
        <item name="colorControlNormal">@color/colorPrimary</item>
        <item name="colorControlActivated">@color/colorGreed</item>
    </style>

    <style name="TextLabel23">
        <!-- Hint color and label color in FALSE state -->
        <item name="android:textColorHint">@android:color/white</item>
        <item name="android:textColorHighlight">@android:color/white</item>
        <item name="android:textColorLink">@android:color/white</item>
        <item name="android:textSize">20sp</item>
        <!-- Label color in TRUE state and bar color FALSE and TRUE State -->
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorControlNormal">@color/colorAccent</item>
        <item name="colorControlActivated">@color/colorAccent</item>
        <!--<item name="android:drawableLeft">@drawable/img_logo</item>-->
    </style>

    <style name="Divider_Vertical">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:background">@color/colorAccent</item>
    </style>

    <style name="Divider_Horizontal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/colorAccent</item>
    </style>
</resources>